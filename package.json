    "@rollup/plugin-commonjs": "^25.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.5",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "rollup": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^14.2.1",
    "tslib": "^2.8.1",
