{
  "name": "mtplot",
  "version": "1.0.0",
  "description": "Minimalistic time series plotting library with pattern detection",
  "main": "dist/mtplot.js",
  "module": "dist/mtplot.esm.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
